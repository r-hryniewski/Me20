  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="CosmosDBEndpoint" value="storedonazure" />
    <add key="CosmosDBKey" value="storedonazure" />
    <add key="CosmosDBDatabaseId" value="storedonazure" />
    <add key="CosmosDBCollectionId" value="storedonazure" />
    <add key="AzureServiceBusConnectionString" value="storedonazure" />
    <add key="BusEndpoint" value="storedonazure" />
  </appSettings>
  <connectionStrings>
    <add name="Me20DbConnectionString" connectionString="STORED IN AZURE" />
  </connectionStrings>
  <akka>
    <hocon>
      <![CDATA[          
            akka 
            {
              actor 
              {
                serializers 
                {
                  hyperion = "Akka.Serialization.HyperionSerializer, Akka.Serialization.Hyperion"
                }
                serialization-bindings 
                {
                  "System.Object" = hyperion
                }
              }
              
              loglevel = INFO,
              loggers = ["Akka.Logger.Serilog.SerilogLogger, Akka.Logger.Serilog"]
            }
      ]]>
    </hocon>
  </akka>
