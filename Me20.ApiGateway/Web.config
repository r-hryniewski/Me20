      </buildProviders>
    </compilation>
    <httpRuntime targetFramework="4.7" />
    <httpHandlers>
      <add verb="*" type="Nancy.Hosting.Aspnet.NancyHttpRequestHandler" path="*" />
    </httpHandlers>
  </system.web>
